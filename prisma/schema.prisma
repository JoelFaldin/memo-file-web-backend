generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model memos {
  id              String          @id @db.VarChar(255)
  rut             String?         @db.VarChar(255)
  direccion       String          @db.VarChar(255)
  tipo            String?         @db.VarChar(255)
  patente         String?         @db.VarChar(255)
  periodo         String?         @db.VarChar(255)
  capital         Decimal?        @db.Decimal
  afecto          Float?
  total           Decimal?        @db.Decimal
  emision         Int?
  giro            String?         @db.VarChar(255)
  agtp            String?         @db.VarChar(255)
  rut_local       String?
  locales         locales?        @relation(fields: [rut_local], references: [rut_local], onDelete: NoAction, onUpdate: NoAction)
  pay_times       pay_times?
  representantes  representantes? @relation(fields: [id], references: [representante_id], onDelete: NoAction, onUpdate: NoAction)
}

model pay_times {
  memo_id String @id @db.VarChar(255)
  day     Int?
  month   Int?
  year    Int?
  memos   memos  @relation(fields: [memo_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model locales {
  rut_local        String      @id @db.VarChar(255)
  nombre_local     String      @db.VarChar(255)
  memos            memos[]
}

model representantes {
  representante_id      String      @id @db.VarChar(255)
  patente               String      @db.VarChar(255)
  rut_representante     String?     @db.VarChar(255)
  nombre_representante  String?
  memos                 memos[]
}
